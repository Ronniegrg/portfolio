{
  "name": "portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://ronniegrg.github.io/portfolio/",
  "scripts": {
    "dev": "node ./node_modules/vite/bin/vite.js --force",
    "build": "node ./node_modules/vite/bin/vite.js build",
    "lint": "eslint .",
    "preview": "node ./node_modules/vite/bin/vite.js preview",
    "clean": "rimraf node_modules/.vite",
    "install-safe": "npm install --no-bin-links",
    "setup": "npm install --no-bin-links && npm run clean",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "deploy-force": "gh-pages -d dist --no-history",
    "deploy-fresh": "node deploy-fresh.js"
  },
  "dependencies": {
    "chart.js": "^4.4.9",
    "date-fns": "^4.1.0",
    "emailjs-com": "^3.2.0",
    "pdfjs-dist": "^5.2.133",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-github-calendar": "^4.5.7",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-pdf": "^9.2.1",
    "react-router-dom": "^7.6.1",
    "react-tooltip": "^5.26.0",
    "vite-plugin-static-copy": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.5.0",
    "eslint": "^9.27.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.2.0",
    "rimraf": "^6.0.1",
    "terser": "^5.40.0",
    "vite": "^6.3.5"
  }
}
